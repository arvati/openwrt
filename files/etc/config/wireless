
config wifi-device 'radio0'
	option type 'mac80211'
	option hwmode '11g'
	option path '10180000.wmac'
	option htmode 'HT40'
	option channel '1'
	option txpower '17'
	option distance '30'
	option country '00'

config wifi-iface 'client'
	option ssid 'casa'
	option encryption 'psk-mixed+tkip+ccmp'
	option device 'radio0'
	option mode 'sta'
	option bssid '90:F6:52:57:57:F0'
	option wds '1'
	option key '1010101010'
	option network 'lan'
	option disabled '1'

config wifi-iface 'service'
	option device 'radio0'
	option mode 'ap'
	option encryption 'none'
	option ssid 'kimax-u35wf'
	option network 'service'
	option wmm '0'
	option disabled '0'

config wifi-iface 'master'
	option device 'radio0'
	option mode 'ap'
	option encryption 'psk-mixed+tkip+ccmp'
	option ssid 'casa'
	option hidden '1'
	option key '1010101010'
	option network 'lan'
	option disabled '1'

